<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="net.worldscale.ee.app.hitaxi.db.tables.User" table="T_USERS">
		<id name="userId" column="F_USER_ID">

		</id>
		<property name="nickname" column="F_NICKNAME"></property>
		<property name="phone" column="F_PHONE"></property>
		<property name="carNumber" column="F_CAR_NUMBER"></property>
	</class>

</hibernate-mapping>
